<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServer</servlet-name>
        <servlet-class>Servlet.LoginServer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUpServer</servlet-name>
        <servlet-class>Servlet.SignUpServer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServer</servlet-name>
        <servlet-class>Servlet.LogoutServer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccountServer</servlet-name>
        <servlet-class>Servlet.AccountServer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddProductServer</servlet-name>
        <servlet-class>Servlet.AddProductServer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CategoriesServer</servlet-name>
        <servlet-class>Servlet.CategoriesServer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AcceptServer</servlet-name>
        <servlet-class>Servlet.AcceptServer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManagerServer</servlet-name>
        <servlet-class>Servlet.ManagerServer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServer</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUpServer</servlet-name>
        <url-pattern>/SignUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServer</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccountServer</servlet-name>
        <url-pattern>/AccountServer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddProductServer</servlet-name>
        <url-pattern>/AddProductServer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CategoriesServer</servlet-name>
        <url-pattern>/CategoriesServer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AcceptServer</servlet-name>
        <url-pattern>/AcceptServer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManagerServer</servlet-name>
        <url-pattern>/ManagerServer</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
